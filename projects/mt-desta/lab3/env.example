# Langfuse Configuration
# After starting Langfuse with docker-compose, create a project in the web UI
# and copy the credentials here

LANGFUSE_PUBLIC_KEY=your-public-key-here
LANGFUSE_SECRET_KEY=your-secret-key-here
LANGFUSE_HOST=http://localhost:3001

# User and Session Tracking
# USER_ID: Unique identifier for the user (optional, defaults to "default-user")
USER_ID=default-user
# SESSION_ID: Unique identifier for the session (optional, auto-generated if not provided)
# SESSION_ID=your-session-id-here

# Environment Configuration
# ENVIRONMENT: Environment name (development, staging, production)
ENVIRONMENT=development

# LLM Configuration (from lab2)
LLM_BASE_URL=http://localhost:11434/api/generate
LLM_MODEL_ID=gemma3:4b

